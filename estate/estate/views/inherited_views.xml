<?xml version="1.0" encoding="UTF-8" ?>

<odoo>

    <data>

        <record id="estate.inherited_form_view" model="ir.ui.view">
            <field name="name">estate.inherited.form.view</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='account_security']" position="after">
<!--                <xpath expr="//sheet" position="inside">-->
                    <page name="available_properties" string="Available Properties">
                        <field name="property_id"/>
                    </page>
                </xpath>
            </field>
        </record>

    </data>

</odoo>